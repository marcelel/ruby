<h1> Wydarzenia</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Artist</th>
      <th>Description</th>
    </tr>
  </thead>

  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= link_to event.id, "/events/#{event.id}" %></td>
        <td><%= link_to event.artist, "/events/#{event.id}" %></td>
        <td><%= link_to event.description, "/events/#{event.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>